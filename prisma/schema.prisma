generator client {
  provider = "prisma-client-js"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model GenerateRoom {

  id             String @id @default(cuid())
  uploadedImage  String
  outputImage    String
  roomStyle      String?
  aiStyle        String? 
  prompt         String
  favourites     String[] @default([])

  userId    String
  userName  String
  userImage String?

  createdAt DateTime @default(now())

}